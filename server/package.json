    "node": ">=14.0.0"
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node dist/server/src/api/server.js",
    "test": "jest",
    "lint": "npx eslint . --ext .js,.ts --fix",
    "nibble": "npx eslint-nibble src --ext .js,.ts",
    "pre-commit": "lint-staged",
    "dev": "ts-node-dev --respawn -r tsconfig-paths/register --cls ./src/api/server.ts",
    "install-all": "npm ci && cd src/frontend && npm ci && cd ../..",
    "start:prod": "NODE_ENV=production node dist/server/src/api/server.js",
    "frontend": "cd src/frontend && npm start",
    "migrate_up_all": "db-migrate up"
  },
  "repository": {
    "type": "git",
